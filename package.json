    "@vue/test-utils": "^1.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^28.0.3",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
